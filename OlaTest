#include <iostream> 
#include <conio.h>
using namespace std;
int main()
{
	int a, b;
	cout << "wprowadz liczby" << endl;
	cin >> a;
	cin >> b;
	cout << a << "+" << b << "=" << a + b << endl;
	cout << a << "-" << b << "=" << a - b << endl;
	cout << a << "*" << b << "=" << a*b << endl;
	cout << a << "/" << b << "=" << a / b << endl;
	_getch();
		return 0;
}
